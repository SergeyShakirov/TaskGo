{
  "name": "taskgo-backend",
  "version": "1.0.0",
  "description": "Backend for TaskGo mobile application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:setup": "ts-node src/scripts/setup-local-db.ts",
    "db:reset": "DB_FORCE_SYNC=true npm run db:setup",
    "db:seed": "ts-node src/utils/seed.ts",
    "db:create": "ts-node src/scripts/create-database.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:postgres:setup": "ts-node src/scripts/setup-postgres.ts",
    "db:docker:up": "docker-compose up -d",
    "db:docker:down": "docker-compose down",
    "db:docker:logs": "docker-compose logs postgres",
    "db:docker:reset": "docker-compose down -v && docker-compose up -d"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "html-pdf-node": "^1.0.8",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "officegen": "^0.6.5",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/html-pdf-node": "^1.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^30.0.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
